{
  "version": "0.2.0",
  "configurations": [
  
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Agents.console/bin/Debug/net9.0/Agents.console.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Agents.console",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": ".NET Core Launch (netcoredbg)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Agents.console/bin/Debug/net9.0/Agents.console.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Agents.console",
      "stopAtEntry": false,
      "console": "internalConsole",
      "pipeTransport": {
        "pipeCwd": "${workspaceFolder}",
        "pipeProgram": "bash",
        "pipeArgs": ["-c"],
        "debuggerPath": "~/netcoredbg/netcoredbg",
        "debuggerArgs": ["--interpreter=vscode"],
        "quoteArgs": true,
      }
    }
  ]
}